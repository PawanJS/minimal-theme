  "bugs": {
    "url": "https://github.com/PawanJS/minimal-theme/issues"
  },
  "homepage": "https://github.com/PawanJS/minimal-theme#readme",
  "devDependencies": {
    "laravel-mix": "^6.0.39"
  }
